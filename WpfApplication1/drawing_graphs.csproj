<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
      <TargetFramework>net5.0-windows</TargetFramework>
      <UseWPF>true</UseWPF>
      <OutputType>WinExe</OutputType>
      <PackageId>drawing_graphs</PackageId>
      <Authors>drawing graphs</Authors>
      <Product>drawing graphs</Product>
      <AssemblyName>drawing_graphs</AssemblyName>
      <RootNamespace>drawing_graphs</RootNamespace>
      <ApplicationIcon>setting_settings_wrench_cogwheel_icon_193940.ico</ApplicationIcon>
  </PropertyGroup>
	<ItemGroup>
    <PackageReference Include="HandyControl" Version="3.2.0" />
    <PackageReference Include="OxyPlot.Wpf" Version="2.0.0" />
	</ItemGroup>
	<ItemGroup>
	  <Compile Update="Properties\Resources.Designer.cs">
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	    <DependentUpon>Resources.resx</DependentUpon>
	  </Compile>
	</ItemGroup>
	<ItemGroup>
	  <EmbeddedResource Update="Properties\Resources.resx">
	    <Generator>ResXFileCodeGenerator</Generator>
	    <LastGenOutput>Resources.Designer.cs</LastGenOutput>
	  </EmbeddedResource>
	</ItemGroup>
</Project>
